#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../target/generated-sources/vendor/apidoc/bin/apidoc" "$@"
  ret=$?
else 
  node  "$basedir/../../target/generated-sources/vendor/apidoc/bin/apidoc" "$@"
  ret=$?
fi
exit $ret
